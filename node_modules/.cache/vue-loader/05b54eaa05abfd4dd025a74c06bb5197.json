     this.autofocus = true;\n        },\n        methods: {\n            handleBlur(value) {\n                this.password = value;\n                if(this.password.length > 1) {\n                    this.isPassword = true\n                } else{\n                    this.isPassword = false\n                }\n            },\n            handleCheckPassword() {\n                this.$emit('handleCheckPassword', this.password)\n            },\n            handleBack() {\n                this.$emit('handleBack')\n            }\n        },\n        destroyed() {\n            this.autofocus = false;\n        }\n    }\n</script>\n"]}]}